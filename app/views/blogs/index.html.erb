<h2 class="display-5 border-bottom pb-2 mt-3 mb-3">メソッド一覧</h1>
<p>メソッド一覧です。</p>


<h2></h2>
<table>
  <tr>
    <th class="col-3 index-th"></th>
    <th class="col-7 index-th">メソッド名</th>
    <th class="col-2 index-th">check</th>
  </tr>

<% @blogs.each do |blog| %>
  <tr>
    <% if @blogs.first.id == blog.id %>
      <td class="col-2 td1"><strong>Rubyメソッド</strong></td>
    <% else %>
      <td class="col-2 td1"></td>
    <% end %>
    <td class="col-9"><%= link_to blog.name + "メソッド", blog %></td>
    <td class="col-1"></td>
  </tr>
<% end %>
</table>

<table>
  <tr>
    <th class="col-3 index-th"></th>
    <th class="col-7 index-th">メソッド名</th>
    <th class="col-2 index-th">check</th>
  </tr>

<% @rails.each do |rail| %>
  <tr>
    <% if @rails.first.id == rail.id %>
      <td class="col-2 td1"><strong>Railsメソッド</strong></td>
    <% else %>
      <td class="col-2 td1"></td>
    <% end %>
    <td class="col-9"><%= link_to rail.name + "メソッド", rail %></td>
    <td class="col-1"></td>
  </tr>
<% end %>
</table>
